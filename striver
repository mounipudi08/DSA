#include<iostream>
using namespace std;
int main() {
cout <<  "Hey Striver!" << endl << "Hey Raj" << endl ;
return 0;
}

#include<iostream>
using namespace std;
int main() {
int x,y;
cin >> x >> y;
cout << "Value of x : " << x << "and y" << y; 
return 0;
}

to include all libraries --> we use this #include<bits/stdc++.h>

 #include<bits/stdc++.h>
using namespace std;

int main() {
int x = 10;
long x = 15;
long long x = 1500000000;

float x = 5.6;
double y = 5;
cout << "Value of  "<< y
return 0;
}

